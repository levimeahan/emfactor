{
  "compilerOptions": {
    "target": "es5",
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "./dist",
    "lib": [
      "esnext"
    ]
  },
  "include": [
    "src"
  ]
}
